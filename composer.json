{
  "name": "ps-jf/postcode-anywhere",
  "description": "Laravel API wrapper for PCA Predict formerly Postcode Anywhere",
  "keywords": [
    "postcode anywhere",
    "postcode lookup",
    "address lookup",
    "find and retrieve"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jonathan Foley",
      "email": "jonathan.foley@policyservices.co.uk",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "laravel/framework": "^6"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "PostcodeAnywhere\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
        "providers": [
            "PostcodeAnywhere\\PAServiceProvider"
        ],
        "aliases": {
            "PA": "PostcodeAnywhere\\PAFacade"
        }
    }
  }
}
